        </rowConstraints>
         <children>
            <Button fx:id="task1Button" mnemonicParsing="false" onAction="#startTask1" text="Start Task 1" />
            <Button fx:id="task2Button" mnemonicParsing="false" onAction="#startTask2" text="Start Task 2" GridPane.columnIndex="1" />
            <Button fx:id="task3Button" mnemonicParsing="false" onAction="#startTask3" text="Start Task 3" GridPane.columnIndex="2" />
            <Label fx:id="task1Label" text="STOPPED" GridPane.rowIndex="1" />
